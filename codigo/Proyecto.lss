                                 ;
                                 ;setup DDR/IO
                                 ;
                                 ;set pullups, DDRC output
000046 934f                      	PUSH	TEMP
000047 ef4f                      	ldi    	temp, 0xff
000048 b947                      	out    	DDRC,temp
000049 b948                      	out    	PORTC,temp
                                 
                                 ;
                                 ;setup speed etc. of i2c port
                                 ;
                                 ;400Khz=8MHz/(16+2*TWBR*CLKPRS) TWBR and clock presecaler set
                                 ;i2c freq.  clock prescaler default is 1, and is set in
                                 ;TWSR
00004a e448                      	ldi    	temp,72
00004b 9340 00b8                 	sts    	TWBR,temp
                                 
00004d e040                      	ldi    	temp,0x00
00004e 9340 00b9                 	sts    	TWSR,temp
000050 914f                      	POP 	TEMP
000051 9508                      	RET
                                 
                                 ;-SEND I2C START
                                 ;-This will send a I2C_DATA out as the address
                                 ;-sends start condition and address-
                                 I2C_START:
000052 934f                      	PUSH 	TEMP
000053 ea44                      	ldi    	temp, (1<<TWINT)|(1<<TWSTA)|(1<<TWEN)
000054 9340 00bc                 	sts     TWCR,temp
                                 
                                 ;wait for start condition to be sent.  when TWINT in TWCR is cleared, it is sent
                                 WAIT_START:
000056 9140 00bc                 	lds     temp,TWCR
000058 ff47                      	sbrs    temp,TWINT
000059 cffc                      	rjmp    WAIT_START
                                 ;check TWSR for bus status.
                                 ;andi masks last three bits, which are 2=? 1:0prescaler value
00005a 9140 00b9                 	lds     temp,TWSR
00005c 7f48                      	andi    temp,0b11111000
00005d 3048                      	cpi     temp,START
00005e f009                      	breq    PC+2
00005f 940c 0045                 	jmp     errloop
000061 914f                      	POP 	TEMP
000062 9508                      	ret
                                 
