00006c 9140 00bc                 	lds     temp,TWCR
00006e ff47                      	sbrs    temp,TWINT
00006f cffc                      	rjmp    WAIT_DONE
000070 914f                      	POP 	TEMP
000071 9508                      	ret
                                 
