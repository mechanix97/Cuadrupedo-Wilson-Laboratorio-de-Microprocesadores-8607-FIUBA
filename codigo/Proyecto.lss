                                 I2C_PUT:
000063 934f                      	PUSH 	TEMP
                                 NEXT1:
000064 e044                      	ldi     temp,(0<<TWINT) | (1<<TWEN)
000065 9340 00bc                 	sts     TWCR,temp
000067 9350 00bb                 	sts     TWDR,I2C_DATA
000069 e844                      	ldi     temp,(1<<TWINT) | (1<<TWEN)
00006a 9340 00bc                 	sts     TWCR,temp
