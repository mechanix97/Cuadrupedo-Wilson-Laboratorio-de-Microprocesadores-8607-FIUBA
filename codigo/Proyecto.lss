000073 9140 00bc                 	lds     temp,TWCR
000075 ff47                      	sbrs    temp,TWINT
000076 cffc                      	rjmp    WAIT_FOR_BYTE
000077 9150 00bb                 	lds     I2C_DATA,TWDR
000079 914f                      	POP		TEMP
00007a 9508                      	ret
                                 
